.inline-loader
	inline-loader()

.circle-loader
	circle-loader()

@keyframes http-loader
	to
		background-position 30px 0

.t-loading
	background: #eee
	&:before
		circle-loader()
		content: ''
		display: block
		position: absolute
		top: 0
		left: 0
		right: 0
		bottom: 0
		margin: auto
		z-index: 20
